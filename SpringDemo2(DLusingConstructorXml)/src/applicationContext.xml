<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
	"http://www.springframework.org/dtd/spring-beans-2.0.dtd">
	
	<beans>
	<bean id="emp" class="com.model.Employee">
	<constructor-arg value="amit"/>
	<constructor-arg ref="addr"/>
	</bean>
	<bean id="addr" class="com.model.Address">
	<constructor-arg value="Pune"/>
	<constructor-arg value="Maharashtra"/>
	<constructor-arg value="KarveRoad"/>
	</bean>
	</beans>