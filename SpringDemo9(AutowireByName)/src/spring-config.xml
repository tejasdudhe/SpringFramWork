<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
	
<bean id="paddr" class="bean.Address">
<property name="city" value="Pune"/>
<property name="state" value="Maharashtra"/>
</bean>
<bean id="taddr" class="bean.Address">
<property name="city" value="Mumbai"/>
<property name="state" value="Maharashtra"/>
</bean>
<bean id="oaddr" class="bean.Address">
<property name="city" value="Nashik"/>
<property name="state" value="Maharashtra"/>
</bean>
   
<bean id="employee" class="bean.Employee" autowire="byName">
<property name="ename" value="Amit"/>
</bean>
</beans>